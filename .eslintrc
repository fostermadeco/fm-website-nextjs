{
    "extends": [
        "wesbos/typescript",
        "plugin:@next/next/recommended"
    ],
    "rules": {
        "import/no-cycle": "off",
        "no-unsafe-any": "off",
        "no-irregular-whitespace": "off",
        "restrict-template-expressions": "off",
        "no-console": 0,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 4
            }
        ]
    }
}